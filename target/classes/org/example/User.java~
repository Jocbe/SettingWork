@Entity
@Table( name = "Users" )
public class User {
	private String name;
	private QuestionSet[] questionSets;
	private long id;
	
	public User() {}
	
	public User(String n) {
		name = n;
	}
	
	@Id
	@GeneratedValue(generator="increment");
	@GenericGenerator(name="increment", strategy="increment")
	public long getID() {return id;}
	
	
	
}
